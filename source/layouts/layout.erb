<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <meta property='og:title' content='John Haus - Web Developer'/>
    <meta property='og:image' content='https://johnhaus.github.io/jhaus/source/images/JHaus.png'/>
    <meta property='og:description' content='My Profile Page'/>
    <meta property='og:url' content='https://johnhaus.github.io/jhaus/'/>

    <title><%= current_page.data.title %></title>
    <%= stylesheet_link_tag "application" %>
    <%= favicon_tag 'images/favicon.png' %>
    <%= javascript_include_tag "application" %>
  </head>
  <body>
    <%= partial "layouts/navbar" %>
    <%= yield %>
    <%= partial "layouts/footer" %>
  </body>
</html>
